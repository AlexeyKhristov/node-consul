{
  "name": "consul",
  "version": "0.13.0",
  "description": "Consul client",
  "main": "./lib",
  "dependencies": {
    "papi": "^0.19.0"
  },
  "devDependencies": {
    "async": "^0.9.0",
    "debug": "^2.0.0",
    "fixturefiles": "^0.1.0",
    "istanbul": "^0.3.6",
    "jscs": "^1.11.3",
    "jshint": "^2.5.5",
    "lodash": "^3.3.0",
    "mocha": "^2.1.0",
    "nock": "^1.2.0",
    "node-uuid": "^1.4.1",
    "should": "^5.0.1",
    "temp": "^0.8.1"
  },
  "scripts": {
    "cover": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha && open coverage/lcov-report/index.html",
    "test": "./node_modules/.bin/jshint lib test && ./node_modules/.bin/jscs lib test && ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --check-leaks && istanbul check-coverage --statements 100 --functions 100 --branches 100 --lines 100",
    "acceptance": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- test/acceptance --recursive --check-leaks --timeout 15000"
  },
