{
  "name": "consul",
  "version": "0.2.0",
  "description": "Consul client",
  "main": "lib/index.js",
  "dependencies": {
    "debug": "^1.0.2"
  },
  "devDependencies": {
    "async": "^0.9.0",
    "jscs": "^1.5.4",
    "jshint": "^2.5.1",
    "mocha": "^1.20.1",
    "should": "^4.0.4",
    "uuid": "^1.4.1"
  },
  "scripts": {
    "test": "./node_modules/.bin/jshint lib test && ./node_modules/.bin/jscs lib test && ./node_modules/.bin/mocha --recursive --require should -R spec",
    "start": "consul agent -data-dir=$( mktemp -d -t consul ) -server -bootstrap -node=node1 -dc=dc1 -pid-file=consul.pid &> consul.log &",
    "stop": "test -f consul.pid && kill $( cat consul.pid )"
  },
  "keywords": [
    "consul"
  ],
  "repository": "https://github.com/silas/node-consul.git",
  "author": "Silas Sewell <silas@sewell.org>",
  "license": "MIT"
}
